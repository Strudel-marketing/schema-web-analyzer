{
  "name": "schema-web-analyzer",
  "version": "1.0.0",
  "description": "Advanced Schema.org markup analysis tool for websites with @id consistency analysis",
  "main": "api/server.js",
  "scripts": {
    "start": "node api/server.js",
    "dev": "nodemon api/server.js",
    "test": "jest",
    "lint": "eslint api/ dashboard/",
    "docker:build": "docker build -t schema-web-analyzer .",
    "docker:run": "docker run -p 3000:3000 schema-web-analyzer",
    "docker:compose": "docker-compose up --build",
    "docker:compose:prod": "docker-compose -f docker-compose.yml up -d",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "schema",
    "schema.org",
    "seo",
    "structured-data",
    "json-ld",
    "analysis",
    "web-scraping",
    "entity-analysis",
    "consistency-check"
  ],
  "author": "Schema Web Analyzer Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^21.5.2",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.0",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "lodash": "^4.17.21",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/schema-web-analyzer"
  },
  "bugs": {
    "url": "https://github.com/your-org/schema-web-analyzer/issues"
  },
  "homepage": "https://github.com/your-org/schema-web-analyzer#readme"
}
